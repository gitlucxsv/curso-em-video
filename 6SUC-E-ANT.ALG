algoritmo "6suc-e-ant"
var
   numero, suc, ant:inteiro
inicio
      escreva("Digite um número :")
       leia(numero)
        ant <- numero - 1
         suc <- numero + 1
          escreval("O antescessor de :", numero, " é ", ant)
           escreval("O sucessor de ", numero, " é ", suc)

fimalgoritmo